                                            <option value="">Escolha um assunto...</option>
                                            <option value="Dúvida">Dúvida</option>
                                            <option value="Sugestão">Sugestão</option>
                                            <option value="Crítica">Crítica</option>
                                            <option value="Elogio">Elogio</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="line">
                                    <div class="column">
                                        <div class="cell fl">
                                            <label>*Mensagem:</label>
                                        </div>
                                    </div>
                                    <div class="column w-100-pc">
                                        <textarea label="Mensagem" obrigatorio="sim" id="ds_msg_fale_conosco" name="ds_msg" class="w-80-pc" rows="5" placeholder="Escreva aqui a sua mensagem" ></textarea>
                                    </div>
                                </div>
                                <div class="line">
                                    <div class="column">
