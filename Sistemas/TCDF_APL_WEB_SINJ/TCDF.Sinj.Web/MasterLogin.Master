<body class="master-green">
    <div class="page">
        <div class="top">
            <div class="art-top">
                <div class="right-top">
                    <span>
                        <%= TCDF.Sinj.Util.MostrarVersao() %>
                    </span>
                </div>
                <div class="logo">
                    <a title="Página Inicial"><img src="<%= TCDF.Sinj.Util._urlPadrao %>/Imagens/logo_sinj.png" alt="sinj" /></a>
                </div>
                <div class="top-bottom" style="height: 40px;">
                    <div class="top-bottom-left">
                        <h2>SISTEMA INTEGRADO DE NORMAS JURÍDICAS DO DF</h2>
                    </div>
                </div>
            </div>
        </div>
        <div class="body">
            <div class="inner-body">
                <div class="content">
                    <div>
                        <asp:ContentPlaceHolder ID="Body" runat="server">
                        </asp:ContentPlaceHolder>
                    </div>
                </div>
            </div>
        </div>
   </div>
 </body>
